      ],
      "source": [
        "from os import close\n",
        "import re\n",
        "import csv\n",
        "import urllib.request\n",
        "\n",
        "url = \"https://msk.spravker.ru/avtoservisy-avtotehcentry/\"\n",
        "response = urllib.request.urlopen(url)\n",
        "html_content = response.read().decode()\n",
        "\n",
        "pattern = r'(?:class=\"org-widget-header__title-link\">)(?P<name>[^<]+)(?:[\\s\\S]+?)(?:<span class=\"org-widget-header__meta org-widget-header__meta--location\">\\s+)(?P<address>[^\\n]+)(?:[\\s\\S]+?)(?:<dd class=\"spec__value\">)(?P<phone>[\\+]?\\d[^<.]+)(?:[\\s\\S]+?)(?:<dd class=\"spec__value\">)(?P<work_hours>[А-я]+[^<.]+)'\n",
        "regex = re.compile(pattern, re.VERBOSE)\n",
        "\n",
        "match = re.findall(pattern, html_content)\n",
        "\n",
        "# print(match)\n",
        "# print()\n",
        "\n",
        "with open('data.csv', 'w', newline='') as file:\n",
        "  writer = csv.writer(file)\n",
        "  writer.writerow(['Название', 'Адрес', 'Номер телефона', 'Рабочие часы'])\n",
        "\n",
        "  for i in match:\n",
        "      writer.writerow(i)\n",
        "\n",
        "matches = re.finditer(pattern, html_content)\n",
        "\n",
        "for match in matches:\n",
        "  print(\"Название:\", match.group('name'))\n",
        "  print(\"Адрес:\", match.group('address'))\n",
        "  print(\"Номер телефона:\", match.group('phone'))\n",
        "  print(\"Рабочие часы:\", match.group('work_hours'))\n",
        "  print(\"\\n\")\n",
        "\n",
        "if not re.search(pattern, html_content):\n",
        "  print(\"No match found.\")\n"
      ]
    }
  ]
}
